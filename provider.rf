provider "aws" {
  region = "us-west-2"  # Replace with your desired region
}

module "eks_cluster" {
  source = "terraform-aws-modules/eks/aws"

  cluster_name      = "my-eks-cluster"
  cluster_version   = "1.21"
  subnets           = ["subnet-abc123", "subnet-def456"] 
  vpc_id            = "vpc-#"  
  worker_groups = [
    {
      instance_type = "t3.medium"
      asg_max_size  = 5
    },
  ]
}
